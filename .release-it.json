{
  "increment": "conventional:angular",
  "non-interactive": true,
  "preReleaseId": "next",
  "git": {
    "commitMessage": "ci: %s [ci skip]",
    "tagAnnotation": "%s",
    "requireUpstream": false,
    "requireCleanWorkingDir": false
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": true,
    "releaseName": "%s",
    "tokenRef": "GITHUB_TOKEN",
    "draft": false
  },
  "scripts": {
    "changelog": "git log --pretty=format:'* %s (%h)' [REV_RANGE]"
  }
}
